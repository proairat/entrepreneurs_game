    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/line-clamp": "^0.4.2",
    "@tailwindcss/typography": "^0.5.7",
    "@types/lodash": "^4.14.191",
    "@videojs-player/vue": "^1.0.0",
    "element-plus": "^2.2.17",
    "normalize.css": "^8.0.1",
    "pinia": "^2.0.21",
    "vee-validate": "^4.6.10",
    "video.js": "^7.20.3",
    "vue": "^3.2.38",
    "vue-router": "^4.1.5",
    "yup": "^0.32.11"
